ul
  each val in [1, 2, 3]
    li= val

ul
  each val, index in [1, 2]
    li
      span= val
      p= index

ul
  each val in {1: 'one', 2: 'two'}
    li= val

ul
  each val, key in {1: 'one', 2: 'two'}
    li
      span= val
      p= key

ul
  each val in [{'a': 'one', 'b': 'two'}]
    li= val['b']
